<%- include('../partials/_head') -%>
<%- include('../partials/_nav') -%>

<div id="message-board-container">
    <h2>Welcome to the message board</h2>
    <% messages.forEach(function(message) { %>
        <h4><%= message.author %></h3>
        <h3><%= message.title %></h3>
        <p><%= message.content %></p>
    <% }); %>
</div>